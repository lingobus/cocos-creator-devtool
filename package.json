{
  "name": "cocos-creator-devtool",
  "version": "1.0.0",
  "description": "Chrome and DevTools extension for debugging Cocos Creator games",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development WEBPACK=css,html node ./bin/build.js",
    "start": "cross-env HMR=true NODE_ENV=development WEBPACK=all babel-watch app.js",
    "start-nohmr": "cross-env NODE_ENV=development babel-watch app.js",
    "start-onlyjs": "cross-env HMR=true NODE_ENV=development WEBPACK=js babel-watch app.js",
    "release": "cross-env NODE_ENV=production node ./bin/build.js",
    "webpack-watch": "cross-env NODE_ENV=development webpack --watch --config ./bin/webpack.dev.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lingobus/cocos-creator-devtool.git"
  },
  "keywords": [
    "cocos-ceator",
    "devtool",
    "chrome-extension"
  ],
  "author": "aztack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lingobus/cocos-creator-devtool/issues"
  },
  "homepage": "https://github.com/lingobus/cocos-creator-devtool#readme",
